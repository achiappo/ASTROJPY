   "cell_type": "raw",
   "metadata": {},
   "source": [
    "Lnan = np.logical_or(np.isnan(L), np.isinf(L))\n",
    "for par in ['L', 'a', 'b', 'c', 'r']:\n",
    "    exec( \"{p} = {p}[~Lnan]\".format(p=par) )"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {
    "collapsed": true
   },
   "source": [
    "sortL = np.argsort(L)[::-1]\n",
